    <%= form_for(@course) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <br>
      <%= f.label :played, "Have you played it yet?" %>
      <br>
      <%= f.check_box :played %><br>
      <br>
      <%= f.label :category %><br>
      <br>
      <%= f.label :type %>
          <%= f.collection_select(:type_id, Type.all, :id, :style, prompt: "Pick a type or add your own below") %><br>
    <br>
        <%= f.fields_for :type, Type.new do |t| %><br>
            <%= t.label "Create My Own type"%><br>
            <br>
            <%= t.text_field :style %>
        <% end%>
      <br>
      <%= f.submit %>
    <% end %>